#!/home/alex/PycharmProjects/my_crm/crm_env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mr-proper==0.0.6','console_scripts','mr_proper'
__requires__ = 'mr-proper==0.0.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mr-proper==0.0.6', 'console_scripts', 'mr_proper')()
    )
